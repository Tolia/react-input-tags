{
  "name": "react-tagging",
  "version": "1.0.0",
  "description": "React component for tagging inputs",
  "main": "index.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel src -d lib",
    "browserify": "./node_modules/.bin/browserify -t [ babelify ] src/index.js -o lib/index.js",
    "reload": "./node_modules/.bin/budo -H 127.0.0.1 -p 8000 -v -d ./lib src/index.js:static/bundle.js -- -t babelify -p [ browserify-hmr -u http://localhost:8002 -p 8002 ]"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baldwmic/react-tagging.git"
  },
  "author": "Michael Baldwin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/baldwmic/react-tagging/issues"
  },
  "homepage": "https://github.com/baldwmic/react-tagging#readme",
  "dependencies": {
    "babel-cli": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-runtime": "^6.11.6",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-plugin-transform-runtime": "^6.15.0",
    "browserify-hmr": "^0.3.5",
    "budo": "^9.2.1"
  }
